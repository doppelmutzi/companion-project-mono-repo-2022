{
  "name": "package-manager-playground",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "start": "npm-run-all -l --parallel dev start-server",
    "start-server": "yarn workspace @doppelmutzi/server serve",
    "dev": "webpack serve --env env=dev",
    "lint-packages": "yarn workspaces foreach -ptR run lint",
    "upgrade-yarn": "yarn set version latest",
    "list-workspaces": "yarn workspaces list --json"
  },
  "dependencies": {
    "@doppelmutzi/eslint-config": "git@github.com:doppelmutzi/companion-project-mono-repo-2022.git#workspace=@doppelmutzi/eslint-config",
    "@doppelmutzi/hooks": "workspace:*",
    "@doppelmutzi/server": "workspace:*",
    "npm-run-all": "4.1.5",
    "prop-types": "15.8.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.1",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/plugin-proposal-class-properties": "7.16.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-react": "7.16.5",
    "@babel/runtime": "7.16.5",
    "@hot-loader/react-dom": "17.0.1",
    "assets-webpack-plugin": "7.1.1",
    "autoprefixer": "10.4.1",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "2.0.2",
    "chalk": "4.1.2",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.5.1",
    "react-hot-loader": "4.13.0",
    "rimraf": "3.0.2",
    "source-map-loader": "3.0.0",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2",
    "webpack-merge": "5.8.0"
  }
}
